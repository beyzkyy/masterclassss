<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF"
    tools:context=".ogrenciekmeleekrani2">

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="64dp"
        android:onClick="kaydet"
        android:text="kaydet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tarih3" />

    <EditText
        android:id="@+id/dersgunleri3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:ems="10"
        android:hint="ders gunleri"
        android:inputType="textPersonName"
        android:maxLength="10"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/ders3"
        app:layout_constraintTop_toBottomOf="@+id/ders3" />

    <EditText
        android:id="@+id/ders3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:ems="10"
        android:hint="ders"
        android:inputType="textPersonName"
        android:maxLength="20"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/okul3"
        app:layout_constraintTop_toTopOf="@+id/okul3" />

    <EditText
        android:id="@+id/derssaatileri3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:ems="10"
        android:hint="ders saatleri"
        android:inputType="textPersonName"
        android:maxLength="15"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/dersgunleri3"
        app:layout_constraintTop_toBottomOf="@+id/dersgunleri3" />

    <EditText
        android:id="@+id/okul3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="okul"
        android:inputType="textPersonName"
        android:maxLength="35"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/adsoyad3"
        app:layout_constraintTop_toBottomOf="@+id/adsoyad3" />

    <EditText
        android:id="@+id/sınıf3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:ems="10"
        android:hint="sınıf"
        android:inputType="textPersonName"
        android:maxLength="1"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/derssaatileri3"
        app:layout_constraintTop_toTopOf="@+id/derssaatileri3" />

    <EditText
        android:id="@+id/adsoyad3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:hint="ad-soyad"
        android:inputType="textPersonName"
        android:maxLength="35"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintEnd_toStartOf="@+id/button13"
        app:layout_constraintTop_toBottomOf="@+id/button13" />

    <EditText
        android:id="@+id/tarih3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="tarih"
        android:inputType="time"
        android:maxLength="8"
        android:maxLines="1"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="@+id/mail3"
        app:layout_constraintTop_toBottomOf="@+id/mail3"
        android:textColorHint="#332D2D" />

    <EditText
        android:id="@+id/velino3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="veli-no"
        android:inputType="phone"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/telno3"
        app:layout_constraintTop_toBottomOf="@+id/telno3" />

    <EditText
        android:id="@+id/telno3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="tel-no"
        android:inputType="phone"

        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/sınıf3"
        app:layout_constraintTop_toBottomOf="@+id/sınıf3" />

    <EditText
        android:id="@+id/mail3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="mail"
        android:inputType="textEmailAddress"
        android:maxLength="40"
        android:maxLines="1"
        android:textColor="#000000"
        android:textColorHint="#332D2D"
        app:layout_constraintStart_toStartOf="@+id/velino3"
        app:layout_constraintTop_toBottomOf="@+id/velino3" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="37dp"
        android:onClick="geridon"
        android:text="x"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>